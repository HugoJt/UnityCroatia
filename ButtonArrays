using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using UnityEngine.UI;

public class ButtonArray : MonoBehaviour
{
    string[] myStringArray = {"a1", "a2", "a3", "a4"};
    int nbrButton = 4;
    void Start()
    {
        for (int j = 0; j < nbrButton; j++) 
        {
            GameObject.Find("button1").GetComponentInChildren<Text>().text = myStringArray[j];
        }
    }

    // Update is called once per frame
    void Update()
    {
        
    }
}
